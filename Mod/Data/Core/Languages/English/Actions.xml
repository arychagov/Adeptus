    <entry name="SpaceMarines/NullZone" value="<string name='Traits/NullZone'/>" />
    <entry name="SpaceMarines/NullZoneDescription" value="Removes invulnerable, witchfire and feel no pain damage reduction on non-hero units adjacent to Librarian." />
    <entry name="SpaceMarines/NullZoneFlavor" value="<string name='Traits/NullZoneFlavor'/>" />
    <entry name="SpaceMarines/Righteousness" value="<string name='Traits/SpaceMarines/Righteousness'/>" />
    <entry name="SpaceMarines/RighteousnessDescription" value="<string name='Traits/SpaceMarines/RighteousnessDescription'/>" />
    <entry name="SpaceMarines/RighteousnessFlavor" value="<string name='Traits/SpaceMarines/RighteousnessFlavor'/>" />
