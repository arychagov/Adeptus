<?xml version="1.0" encoding="utf-8"?>
<dropList layout="Absolute"
		addToContent="0">
	<contentContainer name="selectedItemContainer"
			preferredSize="FillParent FillParent"
			content.margin="6 4; 26 4"
			content.clipMode="Clipped"
			surface.texture="GUI/Button">
		<label name="label"
				preferredSize="FillParent FillParent"
				wordCut="1"
				interactive="0"
				visible="0"/>
	</contentContainer>
	<image name="arrow" 
			preferredSize="WrapContent WrapContent"
			texture="GUI/Arrow"
			rotation="180"
			interactive="0"/>
	<list name="list"
			preferredSize="FillParent WrapContent"
			surface.texture="GUI/ShadowedSurface"
			content.margin="0 0"
			visibleContentContainer.content.margin="2 2"
			maxSize="0 360"
			showEffect="FadeIn"
			hideEffect="FadeOut"
			highlightOnHover="1"
			highlightOnSelect="1"
			visible="0"/>
</dropList>
