<?xml version="1.0" encoding="utf-8"?>
<slider>
	<imageButton name="downButton"
			preferredSize="30 30"
			image.texture="GUI/Arrow"
			image.preferredSize="WrapContent WrapContent"
			image.rotation="-90"/>
	<progressBar name="progressBar"
			preferredSize="FillParent FillParent"
			weights="1 FillAll"
			startColor="0.25 0.25 0.25 0.5"
			endColor="0.25 0.25 0.25 0.5"/>
	<imageButton name="upButton" 
			preferredSize="30 30"
			image.texture="GUI/Arrow"
			image.preferredSize="WrapContent WrapContent"
			image.rotation="90"/>
</slider>
